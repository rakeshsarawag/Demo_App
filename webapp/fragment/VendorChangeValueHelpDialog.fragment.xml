<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog
		noDataText="{i18n>NO_VENDOR}"
		title="{i18n>SELECT_VENDOR}"
		search="handleVendorSearch"
		confirm="handleVendorConfirm"
		close="handleVendorClose"
		items="{
			path: '/VendorSelectionSet'
		}" >
		<StandardListItem
			title="{VendorName}"
			description="{Company}"
			info="{VendorNo}"
			iconDensityAware="false"
			iconInset="false"
			type="Active" />
	</SelectDialog>
</core:FragmentDefinition>